datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Referral {
  id              Int      @id @default(autoincrement())
  referringPerson String
  referredPerson  String
  course          String
  referringEmail  String
  referredEmail   String
  createdAt       DateTime @default(now())
}
